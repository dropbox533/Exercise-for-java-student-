
/*Задача FizzBuzz(Вывести на экран числа от 1 до 100, при этом если число делится на три,но не делится на 5 то выести Fizz, если число делится
на 5 и не делится на 3 вывести -Buzz, если число делится на 3 и на 5 вывести FizzBuzz. */

public class FizzBuzz {
    public static void main(String[] args) {
        int count = 1;

        for (int i = 1; i < 101; i++) {
            if (count % 3 == 0 && count % 5 != 0)
                { System.out.println("Fizz ");}

            else if (count % 5 == 0 && count % 3 != 0)
             { System.out.println("Buzz "); }

            else if (count % 3 == 0 && count % 5 == 0)
                System.out.println("FizzBuzz ");

            else { System.out.println(count); }
            count++;
        }
    }
}
